<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
    
    <Button name="WishlistItemSlotButton" inherits="ItemButtonTemplate" virtual="true">
        <Scripts>
            <OnMouseDown>
                WishlistItemSlotButton_OnMouseDown(self);
            </OnMouseDown>
            <OnLoad>
                WishlistItemSlotButton_OnLoad(self);
            </OnLoad>
            <OnUpdate>
				CursorOnUpdate(self);
				if ( GameTooltip:IsOwned(self) ) then
					WishlistItemSlotButton_OnEnter(self);
				end
			</OnUpdate>
            <OnEnter>
				WishlistItemSlotButton_OnEnter(self, motion);
			</OnEnter>
            <OnLeave>
				GameTooltip:Hide();
				ResetCursor();
			</OnLeave> 
         <!--   
             <OnEvent> 
				InspectPaperDollItemSlotButton_OnEvent(self, event, ...);
			</OnEvent>
            <OnClick>
				InspectPaperDollItemSlotButton_OnClick(self, button);
			</OnClick>
			
            
            
            -->
        </Scripts>
		<NormalTexture file="Interface\Buttons\UI-Quickslot2">
			<Size>
				<AbsDimension x="64" y="64"/>
			</Size>
			<Anchors>
				<Anchor point="CENTER">
					<Offset>
						<AbsDimension x="0" y="-1"/>
					</Offset>
				</Anchor>
			</Anchors>
		</NormalTexture>
    </Button>

    
    <Frame name="WishlistMainFrame" setAllPoints="true" inherits="BasicFrameTemplateWithInset" movable="true" enableMouse="true">
        <Scripts>
            <OnLoad>
                WishlistMainFrame_OnLoad(self);
                self:RegisterForDrag("LeftButton");
            </OnLoad>
            <OnDragStart>self:StartMoving();</OnDragStart>
            <OnDragStop>self:StopMovingOrSizing();</OnDragStop>
        </Scripts>
        <Size>
            <AbsDimension x="300" y="350" />
        </Size>
        <Anchors>
            <Anchor point ="CENTER" />
        </Anchors>
        <Frames>
			<Button name="WishlistMainhand" inherits="WishlistItemSlotButton">
                <Anchors>
                    <Anchor point="BOTTOM" x="-40" y="10"/>
			    </Anchors>
		    </Button>
            <Button name="WishlistOffhand" inherits="WishlistItemSlotButton">
                <Anchors>
                    <Anchor point="BOTTOM" x="0" y="10"/>
			    </Anchors>
		    </Button>
            <Button name="WishlistSpecialWeapon" inherits="WishlistItemSlotButton">
                <Anchors>
                    <Anchor point="BOTTOM" x="40" y="10"/>
			    </Anchors>
		    </Button>
        </Frames>
	</Frame>

    <Frame name="NewWishedItemList" setAllPoints="true" inherits="BasicFrameTemplateWithInset" movable="true" enableMouse="true" virtual="true">
        <Scripts>
        </Scripts>
        <Size>
            <AbsDimension x="200" y="200" />
        </Size>
    </Frame>
</Ui>